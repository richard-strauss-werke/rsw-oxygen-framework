            <ref name="mei_att.beamSpan.log.attributes"/>
            <ref name="mei_att.beamSpan.vis.attributes"/>
            <ref name="mei_att.beamSpan.ges.attributes"/>
            <ref name="mei_att.beamSpan.anl.attributes"/>
