         <oneOrMore>
            <ref name="rsw_p"/>
         </oneOrMore>
         <optional>
            <attribute name="pwho" ns="http://richard-strauss-ausgabe.de/ns/1.0">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <ref name="rsw_data.encoding-staff"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="pwhen" ns="http://richard-strauss-ausgabe.de/ns/1.0">
