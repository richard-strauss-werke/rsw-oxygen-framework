                     <choice>
                        <ref name="rsw_author"/>
                        <ref name="rsw_editor"/>
                        <ref name="rsw_meeting"/>
                        <ref name="rsw_respStmt"/>
                     </choice>
                     <zeroOrMore>
                        <choice>
                           <ref name="rsw_author"/>
                           <ref name="rsw_editor"/>
                           <ref name="rsw_meeting"/>
                           <ref name="rsw_respStmt"/>
                        </choice>
                     </zeroOrMore>
                     <oneOrMore>
                        <ref name="rsw_title"/>
                     </oneOrMore>
                     <zeroOrMore>
                        <choice>
                           <ref name="rsw_model.ptrLike"/>
                           <ref name="rsw_idno"/>
                           <ref name="rsw_textLang"/>
                           <ref name="rsw_editor"/>
                           <ref name="rsw_respStmt"/>
                        </choice>
                     </zeroOrMore>
                  </group>
                  <group>
                     <oneOrMore>
                        <choice>
                           <ref name="rsw_title"/>
                           <ref name="rsw_model.ptrLike"/>
                           <ref name="rsw_idno"/>
                        </choice>
                     </oneOrMore>
                     <zeroOrMore>
                        <choice>
                           <ref name="rsw_textLang"/>
                           <ref name="rsw_author"/>
                           <ref name="rsw_editor"/>
                           <ref name="rsw_meeting"/>
                           <ref name="rsw_respStmt"/>
                        </choice>
                     </zeroOrMore>
                  </group>
                  <group>
                     <ref name="rsw_authority"/>
                     <ref name="rsw_idno"/>
                  </group>
               </choice>
            </optional>
            <zeroOrMore>
               <ref name="rsw_availability"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="rsw_model.noteLike"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="rsw_edition"/>
               <zeroOrMore>
                  <choice>
                     <ref name="rsw_idno"/>
                     <ref name="rsw_model.ptrLike"/>
                     <ref name="rsw_editor"/>
                     <ref name="rsw_sponsor"/>
                     <ref name="rsw_funder"/>
                     <ref name="rsw_respStmt"/>
                  </choice>
               </zeroOrMore>
            </zeroOrMore>
            <ref name="rsw_imprint"/>
            <zeroOrMore>
               <choice>
                  <ref name="rsw_imprint"/>
                  <ref name="rsw_extent"/>
                  <ref name="rsw_biblScope"/>
               </choice>
            </zeroOrMore>
