                  <ref name="rsw_model.global"/>
               </zeroOrMore>
            </oneOrMore>
         </group>
         <ref name="rsw_att.global.attributes"/>
         <ref name="rsw_att.canonical.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="rsw_titlePart">
      <element name="titlePart">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">enthält einen Untertitel oder einen Teil eines Titels, wie er auf der Titelseite angegeben ist. [4.6. Title Pages]</a:documentation>
         <ref name="rsw_macro.paraContent"/>
         <ref name="rsw_att.global.attributes"/>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="type"
                       a:defaultValue="main">
               <a:documentation>beschreibt die Funktion dieses Titelteils näher.
Empfohlene Werte sind: 1] main; 2] sub(subordinate) ; 3] alt(alternate) ; 4] short; 5] desc(descriptive) </a:documentation>
               <choice>
                  <value>main</value>
                  <a:documentation>main title of the work</a:documentation>
                  <value>sub</value>
                  <a:documentation>(Untertitel des Werks) subtitle of the work</a:documentation>
                  <value>alt</value>
                  <a:documentation>(Alternativer Titel des Werks) alternative title of the work</a:documentation>
                  <value>short</value>
                  <a:documentation>abbreviated form of title</a:documentation>
                  <value>desc</value>
                  <a:documentation>(Kurzbeschreibung des Werks) descriptive paraphrase of the work</a:documentation>
                  <data type="Name"/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
