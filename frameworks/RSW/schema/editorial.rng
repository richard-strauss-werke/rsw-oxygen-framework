            <choice>
               <ref name="rsw_altIdentifier"/>
               <ref name="rsw_msIdentifier"/>
            </choice>
            <zeroOrMore>
               <ref name="rsw_model.headLike"/>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <ref name="rsw_model.pLike"/>
               </oneOrMore>
               <group>
                  <optional>
                     <ref name="rsw_msContents"/>
                  </optional>
                  <optional>
                     <ref name="rsw_physDesc"/>
                  </optional>
                  <optional>
                     <ref name="rsw_history"/>
                  </optional>
                  <optional>
                     <ref name="rsw_additional"/>
                  </optional>
                  <zeroOrMore>
                     <ref name="rsw_msPart"/>
