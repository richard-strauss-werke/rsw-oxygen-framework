                  <value>M</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(männlich) </a:documentation>
                  <value>F</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(weiblich) </a:documentation>
                  <value>O</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(sonstiges) </a:documentation>
                  <value>N</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(keines) </a:documentation>
                  <value>U</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(unbekannt) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
