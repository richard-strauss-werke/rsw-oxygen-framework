            <ref name="rsw_p"/>
         </oneOrMore>
      </element>
   </define>
   <define name="rsw_faith">
      <element name="faith">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the faith, religion, or belief set of a person. [13.3.2.1. Personal Characteristics]</a:documentation>
